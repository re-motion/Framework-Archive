<?xml version="1.0" encoding="utf-8" ?> 
<BocList>
  <Class>
    <remarks>
      <list type="table">
        <listheader>
          <term>Property</term>
          <description>Resource Identifier</description>
        </listheader>
        <item>
          <term>
            PageInfo
          </term>
          <description>
            <para> &lt;BocList Resource ID&gt;:PageInfo </para>
            <para> e.g. auto:MyBocList:PageInfo </para>
          </description>
        </item>
        <item>
          <term>
            FixedColumns[x].ColumnTitle
          </term>
          <description>
            <para> &lt;BocList Resource ID&gt;:FixedColumns:&lt;Column ID&gt;:ColumnTitle </para>
            <para> e.g. auto:MyBocList:FixedColumns:FirstColumn:ColumnTitle </para>
            <para> Applies to all columns. </para>
          </description>
        </item>
        <item>
          <term>
            FixedColumns[x].Label
          </term>
          <description>
            <para> &lt;BocList Resource ID&gt;:FixedColumns:&lt;Column ID&gt;:Label </para>
            <para> e.g. auto:MyBocList:FixedColumns:FirstColumn:Label </para>
            <para> Applies to columns of type <see cref="BocCommandColumnDefinition"/>. </para>
          </description>
        </item>
      </list>
    </remarks>
  </Class>
  <OnCommandClick>
    <param name="column"> 
      The <see cref="BocColumnDefinition"/> whose command was clicked. 
    </param>
    <param name="listIndex">
      An index that indtifies the <see cref="IBusinessObject"/> with which the clicked command is associated.
    </param>
    <param name="businessObject">
      The <see cref="IBusinessObject"/> with which the clicked command is associated.
    </param>
  </OnCommandClick>
</BocList>
