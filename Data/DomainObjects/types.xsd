<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
		xmlns:xs="http://www.w3.org/2001/XMLSchema" 
		xmlns:t="http://www.rubicon-it.com/Data/DomainObjects/Types"
		elementFormDefault="qualified" 
		attributeFormDefault="unqualified"
		targetNamespace="http://www.rubicon-it.com/Data/DomainObjects/Types">

	<xs:simpleType name="requiredStringType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:whiteSpace value="collapse" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="dotNetType">
		<xs:restriction base="t:requiredStringType">
			<xs:pattern value=".+,.+" />
		</xs:restriction>				
	</xs:simpleType>
	
</xs:schema>
