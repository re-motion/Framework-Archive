<?xml version="1.0" encoding="UTF-8"?>
<!--EXTERNAL_PROPERTIES: build.outputdir;build.test.outputdir;project.output;project.extension;project.doc;build.debug;build.csc.define-->
<project xmlns="nant-schema" name="Remotion.Data.DomainObjects.Legacy.CodeGenerator">
  
  <target name="build" description="perform a build">
    <csc  target="library"
          output="${build.outputdir}/${project.output}.${project.extension}"
          doc="${project.doc}"
          debug="${build.debug}"
          define="${build.csc.define}">
      <nowarn>
        <!-- missing XML comment -->
        <warning number="1591" />
        <!-- obsolete warning -->
        <warning number="0618" />
      </nowarn> 
      <sources failonempty="true">
        <include name="*.cs"/>
        <include name="MigrationToReflectionBasedMapping/**/*.cs"/>
        <include name="Properties/**/*.cs" />
        <include name="Sql/**/*.cs"/>
        <exclude name="Properties/AssemblyInfoTemplate.cs" />
      </sources>
      <references failonempty="true">
        <include asis="true" name="${build.outputdir}/Remotion.Core.dll"/>
        <include asis="true" name="${build.outputdir}/Remotion.Data.DomainObjects.dll"/>
        <include asis="true" name="${build.outputdir}/Remotion.Data.DomainObjects.Legacy.dll"/>
        <include asis="true" name="System.dll"/>
        <include asis="true" name="System.Configuration.dll"/>
        <include asis="true" name="System.Data.dll"/>
        <include asis="true" name="System.Xml.dll"/>
      </references>
    </csc>
  </target>
  
</project>
